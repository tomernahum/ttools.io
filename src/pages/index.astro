<style>
    html {
        color-scheme: light dark;
        font-family: Arial, Helvetica, sans-serif;
        font-size: 1.5rem;
        box-sizing: border-box;
    }
    body {
    }
    #main {
        max-width: 36rem;
        margin: auto;
        /* padding: .25rem; */
    }

    h3,
    ul {
        margin-bottom: 0rem;
        margin-top: 0rem;
    }
    li {
        margin-bottom: 0.5rem;
        margin-top: 0.3rem;
    }

    h1 {
        text-decoration: underline;
        width: fit-content;
        padding: 0.1rem 0.25rem;

        /* background-color:rgba(78, 196, 0, 0.3);  */
        background-color: #00ff1a4a;
        /* background-color: #a8ffbbce; */
    }

    @media (prefers-color-scheme: dark) {
        h1 {
            background-color: #3fe1802a;
            background-color: #00ff7743;
        }
    }
</style>
<main id="main">
    <h1>ttools</h1>
    <h3>Online tools</h3>
    <ul>
        <li>
            <a href="https://chat.ttools.io" data-umami-event="go to chat"
                >Realtime Encrypted Chatrooms</a
            >
        </li>
        <li>
            <a
                href="https://universe-splitter.ttools.io"
                data-umami-event="go to universe-splitter"
                >Quantum Universe Splitter</a
            >
